#include<stdio.h>
int main() {
  int row, col;
  scanf("%d %d", &row, &col);
  int a[row][col];

  for (int i = 0; i < row; i++) {
    for (int j = 0; j < col; j++) {
      scanf("%d", &a[i][j]);
    }
  }

  for (int i = 0; i < row; i++) {
    int sum = 0;
    for (int j = 0; j < col; j++) {
      sum += a[i][j];
    }
    printf("%d\n", sum);
  }

  return 0;
}
